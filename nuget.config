<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <packageSources>
    <!--To inherit the global NuGet package sources remove the <clear/> line below -->
    <clear />
    <add key="nuget" value="https://api.nuget.org/v3/index.json" />
    <add key="smalls1652-github" value="https://nuget.pkg.github.com/Smalls1652/index.json" />
  </packageSources>
  <packageSourceMapping>
    <packageSource key="nuget">
      <package pattern="*" />
    </packageSource>
    <packageSource key="smalls1652-github">
      <package pattern="BlazorMD.SourceGenerator" />
    </packageSource>
  </packageSourceMapping>
</configuration>